{
  "name": "inline-json-formatter",
  "displayName": "INLINE JSON FORMATTER",
  "description": "format json in anywhere any file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "inline-json-formatter.formatSelectedJSON",
      "title": "Format JSON"
    },{
      "command": "inline-json-formatter.stringifyJavaScriptObject",
      "title": "Stringify JSON Object"
    },{
      "command": "inline-json-formatter.formatWithCustomSpace",
      "title": "Format JSON with custom space"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
